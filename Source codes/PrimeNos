#include<stdio.h>
int main()
{
int i,j,m,n;
printf("enter the numbers: ");
scanf("%d%d",&n,&m);
for(i=n;i<=m;i++)
{
    if(i==2)
    {
          printf("2, ");
          continue;
    }
    if(i==1)
    continue;
  
    int count = 0;
    for(j=2;j<i;j++)
    {
        if(i%j==0)
        {
            count+=1;
        break;
        }
    }
  
    if(count == 0)
    printf("%d, ",i);
    
}

}




